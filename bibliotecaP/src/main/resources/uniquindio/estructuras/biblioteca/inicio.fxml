<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="640.0" prefWidth="761.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="uniquindio.estructuras.biblioteca.controllers.InicioController">
   <children>
      <Pane fx:id="tittlePane" prefHeight="40.0" prefWidth="768.0" styleClass="tittlePane">
         <children>
            <ImageView fitHeight="31.0" fitWidth="33.0" layoutX="14.0" layoutY="4.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/icon.png" />
               </image>
            </ImageView>
            <ImageView fx:id="hideBtn" fitHeight="22.0" fitWidth="24.0" layoutX="695.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/hide.png" />
               </image>
            </ImageView>
            <ImageView fx:id="closeBtn" fitHeight="22.0" fitWidth="22.0" layoutX="732.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../images/cerrar.png" />
               </image>
            </ImageView>
         </children>
      </Pane>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="192.0" layoutY="82.0" prefHeight="78.0" prefWidth="385.0" text="Bienvenide a la Biblioteca (Titulo sujeto a cambios)">
         <font>
            <Font name="Arial Bold" size="12.0" />
         </font>
      </Label>
      <Button fx:id="btnBibliotecario" layoutX="332.0" layoutY="219.0" mnemonicParsing="false" onMouseClicked="#mouseClicked" text="Soy Bibliotecarie" />
      <Button fx:id="btnEstudiante" layoutX="337.0" layoutY="364.0" mnemonicParsing="false" text="Soy Estudiante" />
   </children>
</AnchorPane>
